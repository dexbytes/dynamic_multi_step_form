        "isMulitpleSelect":false
      },
      "value":"Salaried",
      "validation":{
        "required":true,
        "isReadOnly":false,
        "isDisabled":false
      },
      "valid":false
    },
    {
      "elementType":"select",
      "elementConfig":{
        "name":"College",
        "label":"College",
        "enableLabel":true,
        "placeholder":"College",
        "class":"form-control",
        "options":[
          {
            "value":"ips",
            "displayValue":"IPS"
          },
          {
            "value":"sdbansal",
            "displayValue":"SD Bansal"
          }
        ],
        "isMulitpleSelect":false
      },
      "value":"",
      "validation":{
        "required":true,
        "isReadOnly":false,
        "isDisabled":false
      },
      "valid":false
    },
    {
      "elementType":"input",
      "elementConfig":{
        "type":"tel",
        "name":"tel",
        "label":"Phone",
        "enableLabel":false,
        "placeholder":"Phone",
        "class":"form-control",
        "resetIcon":true,
        "isCountryCode":true,
        "keyboardRejex": "^[0-9]+",
        "enabledCountries" : ["Sa","In"]
      },
      "value":"",
      "validation": {
        "required":true,
        "minLength":8,
        "maxLength":10,
        "rejex":"^[0-9]+",
        "isReadOnly":false,
        "isDisabled":false,
        "errorMessage": {
          "required":"Please enter phone number",
          "minLength":"Minimum 8 digit mobile number required",
          "rejex": "Please enter valid mobile number"
        }
      },
      "valid":true,
      "onchange":true
    }
  ]
}
