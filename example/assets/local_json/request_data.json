{
  "formName" : "Registration",
  "title": "Sign Up",
  "description": "Hey, get on board ",
  "image": "",
  "formType": "single",
  "formFields":[
    {
      "elementType": "input",
      "elementConfig": {
        "type": "text",
        "name" : "first_name",
        "label" : "First name",
        "enableLabel":true,
        "placeholder": "First name",
        "class" : "form-control",
        "keyboardRejex": "^[a-zA-Z-]+",
        "resetIcon": true,
        "minLine":1,
        "maxLine": 1,
        "textCapitalization": "words"
      },
      "value": "",
      "validation": {
        "required": true,
        "minLength": 3,
        "maxLength": 50,
        "rejex": "^[a-zA-Z-]+",
        "isReadOnly": false,
        "isDisabled": false,
        "errorMessage":{
          "required":"Please enter first name",
          "minLength":"Minimum 3 characters require",
          "rejex": "Please enter valid first name"
        }
      },
      "valid": true,
      "onchange":false
    },
    {
      "elementType": "input",
      "elementConfig": {
        "type": "text",
        "name" : "full_name",
        "label" : "Full name",
        "enableLabel":true,
        "placeholder": "Full name",
        "class" : "form-control",
        "keyboardRejex": "^[a-zA-Z\\s-]+",
        "resetIcon": true,
        "minLine":1,
        "maxLine": 1,
        "textCapitalization": "words"
      },
      "value": "",
      "validation": {
        "required": true,
        "minLength": 5,
        "maxLength": 50,
        "rejex": "^[a-zA-Z.a-zA-Z-`]+ [a-zA-Z.a-zA-Z-`]+",
        "isReadOnly": false,
        "isDisabled": false,
        "errorMessage":{
          "required":"Please enter full name",
          "minLength":"Minimum 5 characters require",
          "rejex": "Please enter valid full name"
        }
      },
      "valid": true,
      "onchange":false
    },
    {
      "elementType":"input",
      "elementConfig":{
        "type":"email",
        "name":"email",
        "label":"Email",
        "enableLabel":true,
        "placeholder":"EMail",
        "class":"form-control",
        "resetIcon":true
      },

      "value":"",
      "validation": {
        "required":true,
        "isEmail":true,
        "maxLength":50,
        "rejex":"^[a-zA-Z0-9.a-zA-Z0-9.!#$%&'*+-/=?^_`{|}~]+@[a-zA-Z0-9.a-zA-Z0-9.!#$%&'*+-/=?^_`{|}~]+\\.[a-zA-Z]+",
        "isReadOnly":false,
        "isDisabled":false,
        "isUnique":false,
        "errorMessage": {
          "required":"Please enter email",
          "rejex": "Please enter valid email"
        }
      },
      "valid":true,
      "onchange":false
    },
    {
      "elementType":"input",
      "elementConfig":{
        "type":"tel",
        "name":"office",
        "label":"Phone",
        "enableLabel":true,
        "keyboardRejex": "^[0-9+]+",
        "placeholder":"Phone",
        "class":"form-control",
        "resetIcon":true
      },
      "help":{
        "text":"Please enter your phone number",
        "placement":"below"
      },
      "value":"",
      "validation": {
        "required":true,
        "minLength":8,
        "maxLength":13,
        "rejex":"^[0-9+]+",
        "isReadOnly":false,
        "isDisabled":false,
        "errorMessage": {
          "required":"Please enter phone number",
          "minLength":"Minimum 8 digit mobile number required",
          "rejex": "Please enter valid mobile number"
        }
      },
      "valid":true,
      "onchange":false
    },
    {
      "elementType":"input",
      "elementConfig":{
        "type":"password",
        "name":"password",
        "label":"Password",
        "enableLabel":true,
        "placeholder":"Password",
        "class":"form-control",
        "resetIcon":true
      },
      "value":"",
      "validation": {
        "required":true,
        "minLength":4,
        "maxLength":16,
        "rejex":"^(?=.*?[A-Z])(?=.*?[a-z])(?=.*?[0-9])(?=.*?[!@#$&*~])",
        "isReadOnly":false,
        "isDisabled":false,
        "errorMessage": {
          "required":"Please enter password",
          "minLength":"Minimum 4 characters require",
          "rejex": "Please enter strong password"
        }
      },
      "valid":true,
      "onchange":false
    },
    {
      "elementType":"select",
      "elementConfig":{
        "name":"occupation",
        "label":"Occupation",
        "enableLabel":true,
        "placeholder":"Occupation",
        "class":"form-control",
        "options":[
          {
            "value":"Salaried",
            "displayValue":"Salaried"
          },
          {
            "value":"selfEmployed",
            "displayValue":"Self Employed"
          }
